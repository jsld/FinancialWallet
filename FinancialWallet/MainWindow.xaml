<wpf:Dialog
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:b="http://www.scheshan.com/bootstrap"
        x:Class="FinancialWallet.MainWindow"
        xmlns:wpf="http://github.com/scheshan/bootstrap"
        xmlns:view="clr-namespace:FinancialWallet.Views"
        Title="Financial Wallet: Administrador" MinHeight="650" MinWidth="900">

    <TabControl TabStripPlacement="Left" x:Name="MainTabControl">
        <TabItem Header="Button" x:Name="ButtonTab">
            <view:Buttons/>
        </TabItem>

        <TabItem Header="List" x:Name="ListTab">
            <view:List/>
        </TabItem>

        <TabItem Header="Text" x:Name="TextTab">
            <view:Text/>
        </TabItem>
        
        <TabItem Header="ProgressBar" x:Name="ProgressBarTab">
            <view:ProgressBar/>
        </TabItem>
        <TabItem Header="Panel" x:Name="PanelTab">
            <view:Panel/>
        </TabItem>
        <TabItem Header="Agregar Entidad" x:Name="AgregarEntidadTab">
            <view:AddEmpresaView/>
        </TabItem>
        <TabItem Header="Eventos" x:Name="EventosTab">
            <view:EventosView/>
        </TabItem>
    </TabControl>
</wpf:Dialog>